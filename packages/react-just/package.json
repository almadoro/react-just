{
  "name": "react-just",
  "version": "0.1.3",
  "type": "module",
  "description": "React Server Components without a framework. Powered by Vite",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/almadoro/react-just.git"
  },
  "bugs": {
    "url": "https://github.com/almadoro/react-just/issues"
  },
  "keywords": [
    "vite",
    "vite plugin",
    "plugin",
    "react",
    "react server components",
    "server components",
    "use client",
    "use server"
  ],
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts"
    },
    "./client": {
      "types": "./types/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.cjs"
    },
    "./fizz.node": {
      "types": "./types/fizz.node.d.ts",
      "import": "./dist/fizz.node.mjs",
      "require": "./dist/fizz.node.cjs"
    },
    "./flight.node": {
      "types": "./types/flight.node.d.ts",
      "import": "./dist/flight.node.mjs",
      "require": "./dist/flight.node.cjs"
    },
    "./handle.node": {
      "types": "./types/handle.node.d.ts",
      "import": "./dist/handle.node.mjs",
      "require": "./dist/handle.node.cjs"
    },
    "./vite": {
      "types": "./types/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    }
  },
  "scripts": {
    "build": "rm -rf dist && pnpm check && rollup -c",
    "check": "tsc --noEmit -p tsconfig.build.json"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "astring": "^1.9.0",
    "estree-toolkit": "^1.7.13"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/estree": "^1.0.7",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "esbuild": "^0.25.5",
    "rollup": "^4.43.0",
    "typescript": "^5.9.2",
    "vitest": "^3.1.3"
  },
  "peerDependencies": {
    "react": "~19.1",
    "react-dom": "~19.1",
    "react-server-dom-webpack": "~19.1",
    "vite": ">=6"
  }
}
