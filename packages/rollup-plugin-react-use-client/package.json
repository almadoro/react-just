{
  "name": "rollup-plugin-react-use-client",
  "type": "module",
  "keywords": [
    "rollup-plugin",
    "react",
    "use client"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rm -rf dist && pnpm run build:cjs && pnpm run build:esm && pnpm run build:types",
    "build:cjs": "esbuild src/index.ts --format=cjs --bundle --outfile=dist/index.cjs --platform=node --target=node18 --packages=external",
    "build:esm": "esbuild src/index.ts --format=esm --bundle --outfile=dist/index.mjs --platform=node --target=esnext --packages=external",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:open": "xdg-open coverage/index.html",
    "test:update": "vitest --update"
  },
  "dependencies": {
    "astring": "^1.9.0",
    "estree-toolkit": "^1.7.13"
  },
  "devDependencies": {
    "@types/estree": "^1.0.7",
    "@types/node": "^22.15.3",
    "@vitest/coverage-v8": "3.1.3",
    "esbuild": "^0.25.4",
    "rollup": "^4.40.1",
    "vitest": "^3.1.3"
  }
}
