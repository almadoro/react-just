{
  "name": "rollup-plugin-react-use-server",
  "type": "module",
  "keywords": [
    "rollup-plugin",
    "react",
    "use server"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rm -rf dist && pnpm check && rollup -c && cp src/types.d.ts dist/index.d.ts",
    "check": "tsc --noEmit -p tsconfig.build.json"
  },
  "dependencies": {
    "astring": "^1.9.0",
    "estree-toolkit": "^1.7.13"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/estree": "^1.0.7",
    "@types/node": "^22.15.3",
    "@vitest/coverage-v8": "3.1.3",
    "rollup": "^4.40.1",
    "vitest": "^3.1.3"
  }
}
